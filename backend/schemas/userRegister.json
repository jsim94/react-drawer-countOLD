{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 4,
      "maxLength": 30
    },
    "password": {
      "type": ["string", "null"],
      "minLength": 8,
      "maxLength": 64
    }
  },
  "additionalProperties": false,
  "required": ["username"]
}
